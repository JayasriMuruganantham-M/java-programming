import java.util.Scanner;
public class num {
    public static void main ( String [] args){
        int num=20;
        Scanner scanner=new Scanner(System.in);
        while(num>=0){
            System.out.println(num);
            num--;
        }
        if(num%2==0)
        {
            System.out.println("Number is even ");
        }
        else{
            System.out.println("Number is odd");
        }
    }
}
