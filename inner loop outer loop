import java.util.Scanner;
public class num {
    public static void main ( String [] args){
        Scanner scanner=new Scanner(System.in);
        for( int i=1;i<=2;i++)
        {
        System.out.println("outer loop "+i);
        for( int j=1;j<=3;j++)
        System.out.println("inner loop "+j);
        }
    }
}

