class Main {
    public static void main(String [] args)
    {
        int[][] arr;
        arr= new int[1][3];
        arr[0][0]=2;
        arr[0][1]=3;
        arr[0][2]=4;
        System.out.println(arr[0][0]);
         System.out.println(arr[0][1]);
          System.out.println(arr[0][2]);
        
    }
}

OUTPUT:
2
3
4
